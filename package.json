{
  "name": "enhancr",
  "productName": "enhancr",
  "version": "0.9.2",
  "description": "Interpolation/Upscaling AI",
  "main": "src/app.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "mafiosnik"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./src/assets/enhancr-icon",
        "asar": false
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "enhancr"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    },
    "asar": false
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@ffprobe-installer/ffprobe": "^1.4.1",
    "@supercharge/promise-pool": "^2.3.2",
    "animate.css": "^4.1.1",
    "aspect-ratio": "^2.1.1",
    "axios": "^0.27.2",
    "colors": "^1.4.0",
    "easy-presence": "^1.2.0-oops1",
    "electron-acrylic-window": "^0.5.11",
    "electron-squirrel-startup": "^1.0.0",
    "find-process": "^1.4.7",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^10.1.0",
    "growing-file": "^0.1.3",
    "hls.js": "^1.1.5",
    "log-symbols": "^5.1.0",
    "mediainfo.js": "^0.1.8",
    "mime": "^3.0.0",
    "mv": "^2.1.1",
    "node-ffprobe": "^3.0.0",
    "npm": "^8.19.2",
    "systeminformation": "^5.11.21"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.68",
    "@electron-forge/maker-deb": "^6.0.0-beta.68",
    "@electron-forge/maker-rpm": "^6.0.0-beta.68",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.68",
    "@electron-forge/maker-zip": "^6.0.0-beta.68",
    "electron": "^21.2.0",
    "electron-reloader": "^1.2.3"
  }
}
